{
  "name": "@warhsn/easy-route-vue",
  "version": "1.0.2",
  "description": "A middleware routing system for Vue Router inspired by Laravel routing",
  "type": "module",
  "main": "./dist/easy-route.umd.cjs",
  "module": "./dist/easy-route.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/easy-route.js",
      "require": "./dist/easy-route.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:warhsn/easy-route-vue.git"
  },
  "author": "Warren Hansen",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/warhsn/easy-route-vue/issues"
  },
  "homepage": "https://github.com/warhsn/easy-route-vue#readme",
  "keywords": [
    "vue router middleware",
    "vue router",
    "vuejs",
    "vuejs middleware",
    "vue router guards",
    "vue 3"
  ],
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vue": "^3.3.13",
    "vue-router": "^4.2.5"
  }
}
